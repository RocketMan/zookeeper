<IfModule mod_rewrite.c>
RewriteEngine On
# the following rule is needed for php5-fcgi, as apache does an extra rewrite
#RewriteCond %{ENV:REDIRECT_APIVER} !^$
#RewriteRule .* - [E=APIVER:%{ENV:REDIRECT_APIVER},E=PREFIX:%{ENV:REDIRECT_PREFIX}]
RewriteCond %{REQUEST_URI}::$0 ^(/.+)/(v([0-9]+(\.[0-9]+)*)/.*)::\2$
RewriteRule .* - [E=BASE:%1,E=APIVER:%3,E=PREFIX:%1/v%3/]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^ %{ENV:BASE}/index.php [L]
</IfModule>
